{
  "name": "tutifrutti",
  "version": "1.0.0",
  "description": "Juego multijugador de Tutti Frutti con Node.js y Socket.IO",
  "main": "server.js",
  "scripts": {
    "start": "node server.js",
    "dev": "nodemon server.js",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage",
    "e2e": "playwright test",
    "e2e:headed": "playwright test --headed",
    "lint": "eslint . --ext .js",
    "lint:fix": "eslint . --ext .js --fix",
    "format": "prettier --write \"**/*.{js,json,md}\"",
    "format:check": "prettier --check \"**/*.{js,json,md}\"",
    "online": "node scripts/tunnel.js"
  },
  "keywords": [
    "game",
    "multiplayer",
    "socket.io",
    "tutifruti"
  ],
  "author": "",
  "license": "ISC",
  "dependencies": {
    "connect-mongo": "^5.1.0",
    "cors": "^2.8.5",
    "dotenv": "^16.3.1",
    "express": "^4.17.1",
    "express-session": "^1.17.3",
    "mongoose": "^7.5.0",
    "socket.io": "^4.7.2"
  },
  "devDependencies": {
    "@playwright/test": "^1.54.2",
    "@types/jest": "^29.5.14",
    "eslint": "^8.57.1",
    "jest": "^29.7.0",
    "localtunnel": "^2.0.2",
    "nodemon": "^3.1.10",
    "prettier": "^3.6.2",
    "socket.io-client": "^4.7.2",
    "supertest": "^6.3.4"
  }
}
